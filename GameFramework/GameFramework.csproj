<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<Nullable>disable</Nullable>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
 		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>

		<PostBuildEvent>
			copy "$(MSBuildProjectName).dll" "$(SolutionDir)..\..\Assets\Plugins\UnityGameFramework\Libraries\$(MSBuildProjectName).dll"
			copy "$(MSBuildProjectName).xml" "$(SolutionDir)..\..\Assets\Plugins\UnityGameFramework\Libraries\$(MSBuildProjectName).xml"
			copy "$(MSBuildProjectName).pdb" "$(SolutionDir)..\..\Assets\Plugins\UnityGameFramework\Libraries\$(MSBuildProjectName).pdb"
		</PostBuildEvent>

		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
	</PropertyGroup>

</Project>